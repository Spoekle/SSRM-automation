{
  "configName": "ssConfig.json",
  "width": 900,
  "height": 300,
  "cardCornerRadius": 20,
  "background": {
    "type": "cover",
    "srcField": "versions.0.coverURL",
    "blur": 10,
    "clip": true
  },
  "components": [
    {
      "type": "roundedRect",
      "x": 0,
      "y": 0,
      "width": 900,
      "height": 300,
      "cornerRadius": 50,
      "fillStyle": "transparent"
    },
    {
      "type": "image",
      "srcField": "versions.0.coverURL",
      "x": 20,
      "y": 20,
      "width": 260,
      "height": 260,
      "clip": true,
      "shadow": {
        "color": "rgba(0, 0, 0, 0.5)",
        "offsetX": 10,
        "offsetY": 10,
        "blur": 5
      }
    },
    {
      "type": "roundedRect",
      "x": 300,
      "y": 20,
      "width": 580,
      "height": 180,
      "cornerRadius": 10,
      "fillStyle": "rgba(0, 0, 0, 0.2)"
